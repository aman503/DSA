#include<iostream>
using namespace std;
int lsrch(int a[7],int p)
{
    for(int i=0;i<7;i++)
    {
        if(a[i]==p)
        {
            return i;
        }
    }
    return -1;
}
int main()
{
    int a[7];
    int p;
    cout<<"element to b search ="<<endl;
    cin>>p;
    for(int i= 0;i<7;i++)
    {
        cin>>a[i];
    }
    int q;
    q=lsrch(a,p);
    if(q!=1)
    cout<<q;
    else
    cout<<"not found";
}
